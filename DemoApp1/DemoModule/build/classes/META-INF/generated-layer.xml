<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN"
                            "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="xml">
        <folder name="entities">
            <folder name="org_netbeans_demo_demomodule">
                <file name="Demo" url="nbres:/org/netbeans/modules/settings/resources/properties-1_0.dtd">
                    <!--org.netbeans.demo.demomodule.DemoTopComponent-->
                    <attr name="hint.originalPublicID" stringvalue="-//org.netbeans.demo.demomodule//Demo//EN"/>
                </file>
            </folder>
        </folder>
        <folder name="memory">
            <folder name="org">
                <folder name="netbeans">
                    <folder name="demo">
                        <folder name="demomodule">
                            <file name="DemoTopComponent">
                                <!--org.netbeans.demo.demomodule.DemoTopComponent-->
                                <attr name="settings.providerPath" stringvalue="xml/lookups//org_netbeans_demo_demomodule/Demo.instance"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
            </folder>
        </folder>
        <folder name="lookups">
            <folder name="org_netbeans_demo_demomodule">
                <file name="Demo.instance">
                    <!--org.netbeans.demo.demomodule.DemoTopComponent-->
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.create" name="instanceCreate"/>
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.properties" name="settings.convertor"/>
                    <attr name="settings.instanceClass" stringvalue="org.netbeans.demo.demomodule.DemoTopComponent"/>
                    <attr name="settings.instanceOf" stringvalue="org.netbeans.demo.demomodule.DemoTopComponent"/>
                    <attr boolvalue="true" name="xmlproperties.preventStoring"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="org-netbeans-demo-demomodule-DemoTopComponent.shadow">
                <!--org.netbeans.demo.demomodule.DemoTopComponent-->
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-demo-demomodule-DemoTopComponent.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="DemoTopComponent.settings">
                <!--org.netbeans.demo.demomodule.DemoTopComponent--><![CDATA[<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE settings PUBLIC "-//NetBeans//DTD Session settings 1.0//EN" "http://www.netbeans.org/dtds/sessionsettings-1_0.dtd">
<settings version="1.0">
  <instance class="org.netbeans.demo.demomodule.DemoTopComponent"/>
</settings>
]]></file>
        </folder>
        <folder name="Modes">
            <folder name="explorer">
                <file name="DemoTopComponent.wstcref">
                    <!--org.netbeans.demo.demomodule.DemoTopComponent--><![CDATA[<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tc-ref PUBLIC "-//NetBeans//DTD Top Component in Mode Properties 2.0//EN" "http://www.netbeans.org/dtds/tc-ref2_0.dtd">
<tc-ref version="2.0">
  <tc-id id="DemoTopComponent"/>
  <state opened="true"/>
</tc-ref>
]]></file>
            </folder>
        </folder>
    </folder>
    <folder name="Actions">
        <folder name="Window">
            <file name="org-netbeans-demo-demomodule-DemoTopComponent.instance">
                <!--org.netbeans.demo.demomodule.DemoTopComponent-->
                <attr
                    methodvalue="org.openide.windows.TopComponent.openAction" name="instanceCreate"/>
                <attr name="component" newvalue="org.netbeans.demo.demomodule.DemoTopComponent"/>
                <attr name="preferredID" stringvalue="DemoTopComponent"/>
                <attr
                    bundlevalue="org.netbeans.demo.demomodule.Bundle#CTL_DemoAction" name="displayName"/>
            </file>
        </folder>
    </folder>
</filesystem>
